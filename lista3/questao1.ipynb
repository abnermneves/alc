    "import numpy as np\n",
    "import math"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "def funcao(m):\n",
    "    W = np.random.randn(m, 4)\n",
    "    nW = np.divide(W, math.sqrt(m))\n",
    "    Z = nW.T@nW\n",
    "    print(Z)\n",
    "    fro = np.linalg.norm(Z - np.eye(4), 'fro')\n",
    "    return fro"
   ]
  },
